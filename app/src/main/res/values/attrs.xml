<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="AlignTextView">
        <attr name="alignOnlyOneLine" format="boolean" />
    </declare-styleable>

    <declare-styleable name="AliPayLoadingView">
        <attr name="rotateColor" format="color" />
        <attr name="circleColor" format="color" />
        <attr name="strokeWidth" format="dimension" />
        <attr name="rotateDuration" format="integer" />
    </declare-styleable>

    <declare-styleable name="ripple_view">
        <attr name="rippleColor" format="color"/>
        <attr name="ripple_type" format="enum">
            <enum name="fill" value="0"/>
            <enum name="stroke" value="1"/>
        </attr>
        <attr name="radius" format="integer"/>
        <attr name="stroke_width" format="integer"/>
    </declare-styleable>

    <declare-styleable name="pager_indicator">
        <attr name="indicatorColor" format="color"/>
        <attr name="verticalPadding" format="dimension"/>
        <attr name="horizontalPadding" format="dimension"/>
        <attr name="android:textSize" format="dimension"/>
    </declare-styleable>

    <attr name="entrySummaries" format="reference" />

    <declare-styleable name="IjkListPreference">
        <attr name="entrySummaries" />
    </declare-styleable>
</resources>